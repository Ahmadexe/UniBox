<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/background"
    tools:context=".GikiWindow">

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="109dp"
        android:layout_height="109dp"
        android:layout_marginTop="50dp"
        android:layout_marginBottom="16dp"
        android:onClick="applicationGiki"
        app:layout_constraintBottom_toTopOf="@+id/hecRankGiki"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/gikiicm" />

    <ImageView
        android:id="@+id/intRankGiki"
        android:layout_width="200dp"
        android:layout_height="275dp"
        android:layout_marginTop="175dp"
        android:layout_marginBottom="8dp"
        android:onClick="intRankingQSGiki"
        app:layout_constraintBottom_toTopOf="@+id/meritCalGiki"
        app:layout_constraintEnd_toStartOf="@+id/hecRankGiki"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/irank" />

    <ImageView
        android:id="@+id/hecRankGiki"
        android:layout_width="213dp"
        android:layout_height="275dp"
        android:layout_marginBottom="8dp"
        android:onClick="hecRankingGiki"
        app:layout_constraintBottom_toTopOf="@+id/locationGiki"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/intRankGiki"
        app:layout_constraintTop_toBottomOf="@+id/imageView5"
        app:srcCompat="@drawable/hrank" />

    <ImageView
        android:id="@+id/meritCalGiki"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="1dp"
        android:layout_marginBottom="6dp"
        android:onClick="calculatorOpenerGiki"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/locationGiki"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/intRankGiki"
        app:srcCompat="@drawable/mcal" />

    <ImageView
        android:id="@+id/locationGiki"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="2dp"
        android:layout_marginBottom="1dp"
        android:onClick="searchMapGiki"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/meritCalGiki"
        app:layout_constraintTop_toBottomOf="@+id/hecRankGiki"
        app:srcCompat="@drawable/loc" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="94dp"
        android:layout_height="27dp"
        android:text=" Apply Now!"
        android:textColor="#808080"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/imageView5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>